<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    android:orientation="horizontal"
    tools:context="proyecto2.app1.com.example.pc.c175.EngineFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        android:background="@color/colorPrimaryDark">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="131dp"
            android:layout_height="29dp"
            android:text="ENGINE TURBO"
            android:textSize="18dp"
            android:textStyle="bold"
            android:gravity="center"
            android:textColor="#ffc400"
            android:layout_marginTop="8dp"
            android:layout_gravity="center"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:background="@color/colorPrimaryDark">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="105dp"
                    android:layout_height="20dp"
                    android:text="TURBO CHARGER #1"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:textColor="#ffc400"
                    android:gravity="center"
                    android:layout_gravity="center"/>

                <SeekBar
                    android:id="@+id/seekBar2"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    android:max="100"
                    android:progress="1"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:text="TURBO CHARGER #3"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textColor="#ffc400"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"/>

                <SeekBar
                    android:id="@+id/seekBar3"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    android:max="10"
                    android:progress="3"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:background="@color/colorPrimaryDark">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="105dp"
                    android:layout_height="20dp"
                    android:text="TURBO CHARGER #2"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:textColor="#ffc400"
                    android:gravity="center"
                    android:layout_gravity="center" />

                <SeekBar
                    android:id="@+id/seekBar4"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    android:max="100"
                    android:progress="1"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:text="TURBO CHARGER #4"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:textColor="#ffc400"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"/>

                <SeekBar
                    android:id="@+id/seekBar5"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    android:max="10"
                    android:progress="3"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="208dp"
            android:layout_height="46dp"
            android:text="COMPRESSOR INLET PRESSURES"
            android:textSize="18dp"
            android:textStyle="bold"
            android:gravity="center"
            android:textColor="#ffc400"
            android:layout_marginTop="8dp"
            android:layout_gravity="center"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:text="ATMOSPHERIC PRESSURE"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:textColor="#ffc400"
                    android:gravity="center"
                    android:layout_gravity="center"/>

                <SeekBar
                    android:id="@+id/seekBar6"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    android:max="10"
                    android:progress="3"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:text="INTAKE MANIFOLD PRESSURE #2"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textColor="#ffc400"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"/>

                <SeekBar
                    android:id="@+id/seekBar7"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:background="#212121"
                    android:max="10"
                    android:progress="3"
                    android:rotation="270"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@color/colorPrimaryDark"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="105dp"
                    android:layout_height="24dp"
                    android:text="INTAKE MANIFOLD PRESSURE #1"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textColor="#ffc400"
                    android:layout_gravity="center"/>

                <SeekBar
                    android:id="@+id/seekBar"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:max="10"
                    android:progress="3"
                    android:rotation="270"
                    android:background="#212121"
                    android:layout_gravity="center"
                    android:layout_marginTop="40dp"/>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>




</LinearLayout>
