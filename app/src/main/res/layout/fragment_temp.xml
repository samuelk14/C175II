        android:id="@+id/seekBar9"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:background="#212121"
        android:rotation="270"
        android:max="10"
        android:progress="3"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
